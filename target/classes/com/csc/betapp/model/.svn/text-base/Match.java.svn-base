package com.csc.betapp.model;

import java.util.Date;

/**
 * @author PH
 *
 */
public class Match {
	private int id;
	private int team1Id;
	private int team2Id;
	private int teamSelected; // team 1 selected: matchRate > 0, else team 2
								// selected: matchRate < 0
	private double matchRate;
	private Date matchTime;
	private Integer scoreTeam1;
	private Integer scoreTeam2;
	private MatchType matchType;

	private String team1Name;
	private String team2Name;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getTeam1Id() {
		return team1Id;
	}

	public void setTeam1Id(int team1Id) {
		this.team1Id = team1Id;
	}

	public int getTeam2Id() {
		return team2Id;
	}

	public void setTeam2Id(int team2Id) {
		this.team2Id = team2Id;
	}

	public double getMatchRate() {
		return matchRate;
	}

	public void setMatchRate(double matchRate) {
		this.matchRate = matchRate;
	}

	public Date getMatchTime() {
		return matchTime;
	}

	public void setMatchTime(Date matchTime) {
		this.matchTime = matchTime;
	}

	public Integer getScoreTeam1() {
		return scoreTeam1;
	}

	public void setScoreTeam1(Integer scoreTeam1) {
		this.scoreTeam1 = scoreTeam1;
	}

	public Integer getScoreTeam2() {
		return scoreTeam2;
	}

	public void setScoreTeam2(Integer scoreTeam2) {
		this.scoreTeam2 = scoreTeam2;
	}

	public MatchType getMatchType() {
		return matchType;
	}

	public void setMatchType(MatchType matchType) {
		this.matchType = matchType;
	}

	public String getTeam1Name() {
		return team1Name;
	}

	public void setTeam1Name(String team1Name) {
		this.team1Name = team1Name;
	}

	public String getTeam2Name() {
		return team2Name;
	}

	public void setTeam2Name(String team2Name) {
		this.team2Name = team2Name;
	}

	public int getTeamSelected() {
		return teamSelected;
	}

	public void setTeamSelected(int teamSelected) {
		this.teamSelected = teamSelected;
	}
}
